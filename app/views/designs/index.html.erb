<h3>Designs</h3>

<div id="indexDiv">
  <table class="table table-striped">
    <tr>
      <th>#</th>
      <th>Part</th>
      <th>Protocol</th>
      <th>Result</th>
    </tr>

    <% @designs.each do |design| %>
      <tr>
        <td><%= design.id %></td>
        <td><%= design.part.name %></td>
        <td><%= design.protocol.name %></td>
        <td><%= link_to 'View', design_path(design.id), {:style => 'color: #049cdb;'} %></td>
      </tr>
    <% end %>
  </table>
</div>
<br />

<%= link_to 'New Design', new_design_path, {:style => 'color: #049cdb;'} %>
<br><br>
